{
  "name": "node-opcua",
  "version": "0.0.61",
  "description": "pure nodejs OPCUA SDK",
  "homepage": "http://node-opcua.github.io/",
  "main": "bundle.js",
  "bin": {},
  "directories": {
    "test": "test"
  },
  "scripts": {
    "_x_preinstall": "npm i browserify -g",
    "postinstall": "node bin/crypto_create_CA.js demo --silent",
    "pretest": "node bin/crypto_create_CA.js demo --dev --silent && mkdir -p tmp",
    "test": "mocha test --color -R spec --recursive --timeout 200000 --bail --compilers js:babel-core/register",
    "lint": "eslint lib",
    "fix": "eslint --fix lib",
    "generate_doc": "make doc",
    "generate_autodoc": "./node_modules/autodoc/bin/autodoc -o _tmp_generated_doc bin/binaryStream.js",
    "build": "babel-node bin/generate_opcua_classes.js --clear",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-opcua/node-opcua.git"
  },
  "keywords": [
    "OPCUA",
    "opcua",
    "m2m",
    "iot",
    "opc ua",
    "internet of things"
  ],
  "author": "Etienne Rossignon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-opcua/node-opcua/issues"
  },
