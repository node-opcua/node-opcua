    "coveralls": "2.11.15",
    "csv": "1.1.0",
    "cucumber": "^2.0.0-rc.2",
    "enum": "2.3.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eyes": "0.1.8",
    "glob-fs": "^0.1.6",
