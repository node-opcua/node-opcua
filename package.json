{
  "name": "node-opcua",
  "version": "0.2.2",
  "private": true,
  "description": "pnure nodejs OPCUA SDK",
  "homepage": "http://node-opcua.github.io/",
  "main": "index.js",
  "bin": {},
  "directories": {
    "test": "test"
  },
  "scripts": {
    "_x_preinstall": "npm i browserify -g",
    "build": "node  ./node_modules/lerna/bin/lerna.js run build",
    "generate": "node  ./node_modules/lerna/bin/lerna.js run generate",
    "bootstrap": "node  ./node_modules/lerna/bin/lerna.js bootstrap ",
    "postinstall": "npm run bootstrap && npm run build && npm run generate",
    "pretest": "node  ./node_modules/lerna/bin/lerna.js run pretest",
    "_old_test": "cd source && mocha test --color -R spec --recursive --timeout 200000 --bail",
    "test": "cd packages && node --expose-gc --max-old-space-size=1024 run_all_mocha_tests.js",
    "test:withnode  ./node_modules/lerna/bin/lerna.js": "node  ./node_modules/lerna/bin/lerna.js run test",
    "eslint": "eslint packages/*/src/**.js packages/*/test/ packages/*/source/ packages/*/test_helpers/ ",
    "eslint-fix": "eslint --fix packages/*/src/**.js packages/*/__tests__/ utils/*.js scripts/*.js",
    "generate_doc": "make doc",
    "generate_autodoc": "./node_modules/autodoc/bin/autodoc -o _tmp_generated_doc bin/binaryStream.js",
    "ncu": "ncu -au && node  ./node_modules/lerna/bin/lerna.js exec ncu -- -au",
    "prettier-watch": "onchange \"packages/*/(src|test|source)/**/*.(js|ts)\" -v -p -- prettier --write {{changed}}",
    "lebab-let": "node  ./node_modules/lerna/bin/lerna.js exec lebab  --replace index.js  --transform let && node  ./node_modules/lerna/bin/lerna.js exec lebab  --replace test  --transform let && node  ./node_modules/lerna/bin/lerna.js exec lebab  --replace src  --transform let"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-opcua/node-opcua.git"
  },
  "keywords": [
    "OPCUA",
    "opcua",
    "m2m",
    "iot",
    "opc ua",
    "internet of things"
  ],
  "author": "Etienne Rossignon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-opcua/node-opcua/issues"
  },
  "devDependencies": {
    "@types/node": "^10.0.4",
    "app-module-path": "^2.2.0",
    "autodoc": "0.6.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.6.1",
    "babili-webpack-plugin": "^0.1.2",
    "benchmark": "2.1.4",
    "bignumber.js": "7.0.1",
    "coffee-script": "^1.12.7",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "3.0.1",
    "csv": "3.0.2",
    "cucumber": "^4.2.1",
    "enum": "2.5.0",
    "eslint": "^4.19.1",
    "eyes": "0.1.8",
    "jsdoc": "3.5.5",
    "litpro": "2.0.0",
    "mocha": "5.1.1",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "^11.7.1",
    "request": "2.85.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "should": "13.2.1",
    "sinon": "5.0.7",
    "tmp": "0.0.33",
    "tracer": "0.8.15",
    "typescript": "^2.8.3",
    "webpack": "^4.8.1",
    "webpack-node-externals": "^1.7.2",
    "yuidoc-bootstrap-theme": "*",
    "yuidocjs": "^0.10.2"
  },
  "typings": "./packages/node-opcua/node-opcua.d.ts",
  "dependencies": {
    "async": "2.6.0",
    "backoff": "2.5.0",
    "better-assert": "*",
    "bomstrip": "0.1.4",
    "byline": "5.0.0",
    "colors": "1.2.4",
    "date-utils": "^1.2.21",
    "deassertify": "^0.1.2",
    "delayed": "^1.0.1",
    "dequeue": "^1.0.5",
    "easy-table": "^1.1.1",
    "exit": "^0.1.2",
    "fqdn": "0.0.3",
    "hexy": "0.2.11",
    "humanize": "0.0.9",
    "lerna": "^2.11.0",
    "ltx": "^2.7.1",
    "mocha-clean": "^1.0.0",
    "node-jsrsasign": "0.0.7",
    "node-opcua-crypto": "0.0.17",
    "node-opcua-pki": "0.0.30",
    "once": "^1.4.0",
    "progress": "2.0.0",
    "q": "^1.5.1",
    "set-prototype-of": "^1.0.0",
    "sprintf": "0.1.5",
    "thenify": "^3.3.0",
    "treeify": "1.1.0",
    "underscore": "^1.9.0",
    "xml-writer": "^1.7.0",
    "yargs": "^11.0.0"
  }
}
